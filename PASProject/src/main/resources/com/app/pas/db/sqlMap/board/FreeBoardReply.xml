<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" 
"http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="FreeBoardReply">

	<typeAlias alias="str" type="java.lang.String" />
	<typeAlias alias="int" type="java.lang.Integer" />
	<typeAlias alias="FreeBoardReply" type="com.app.pas.dto.board.FreeBoardReplyVo" />



	<select id="selectFreeBoardReply" parameterClass="int"
		resultClass="FreeBoardReply">
		SELECT *
		FROM FREE_BOARD_REPLY
		WHERE PRB_ARTICLE_NUM LIKE '%'||#prb_Article_Num#||'%'


	</select>

	<insert id="insertFreeBoardReply" parameterClass="FreeBoardReply">
		INSERT INTO
		FREE_BOARD_REPLY(PRB_REPLY_NUM,PRB_ARTICLE_NUM,PRB_REPLY_MEM,
		PRB_REPLY_CONTENT) 
		VALUES(FREE_BOARD_REPLY_SEQ.NEXTVAL,#prb_Article_Num#,#prb_Reply_Mem#,#prb_Reply_Content#)
	</insert>


	<update id="updateFreeBoardReply" parameterClass="FreeBoardReply">
		UPDATE FREE_BOARD_REPLY SET PRB_REPLY_CONTENT=#prb_Reply_Content# 
		WHERE PRB_REPLY_NUM = #prb_Reply_Num#
	</update>

	<delete id="deleteFreeBoradReply" parameterClass="int">
		DELETE FROM FREE_BOARD_REPLY WHERE PRB_REPLY_NUM=#prb_Reply_Num#


	</delete>

</sqlMap>