<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" 
"http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="WeeklyCheck">

	<typeAlias alias="str" type="java.lang.String" />
	<typeAlias alias="int" type="java.lang.Integer" />

	<select id="weeklyCheck_Select" resultClass="WeeklyCheck">
		SELECT * FROM WEEKLY_CHECK
	</select>
	
	<update id="weeklyCheck_Update" parameterClass="WeeklyCheck">
		INSERT INTO
		WEEKLY_CHECK(WK_NUM,WK_DATE,WK_CONTENT,WK_MEM_EMAIL,WK_YN)
		VALUES(WEEKLY_CHECK_SEQ.NEXTVAL,#wk_Date#,#wk_Content#,#wk_Mem_Email#,#wk_Yn#)
	</update>


</sqlMap>