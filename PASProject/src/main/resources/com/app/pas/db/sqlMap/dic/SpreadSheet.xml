<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" 
"http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="SpreadSheet">

   <typeAlias alias="str" type="java.lang.String" />
   <typeAlias alias="int" type="java.lang.Integer" />
   
	 <update id="insertSpreadSheet" parameterClass="SpreadSheet">
      INSERT INTO
      SPREAD_SHEET(SP_CONTENT,DIC_NUM)
      VALUES(#sp_Content#,#dic_Num#)
   </update>

	<select id="selectSpreadSheetByDocNum" parameterClass="int" resultClass="SpreadSheet">
		select * from spread_sheet where dic_num = #doc_Num#
	</select>
</sqlMap>